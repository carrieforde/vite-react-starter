{
  "bracketSameLine": false,
  "bracketSpacing": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "**/*.svg",
      "options": {
        "parser": "html"
      }
    }
  ]
}
